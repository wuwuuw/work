def count_n(s):
    num=0
    alpha=0
    blank=0
    other=0
    for i in s:
        if i.isdigit():
            num +=1
        elif i.isalpha():
            alpha += 1;
        elif i==' ':
            blank +=1
        else:
            other +=1
    result='共有数字{}个，字母{}个空格{}个，其他{}个'.format (num,alpha,blank,other)
    return(result)
print(count_n(' ssssss66@'))



