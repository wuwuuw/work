import random
right = random.randint(1,100)
print('------猜数字游戏！-----')
------猜数字游戏！-----
number = 0
while number != right:
    number = int(input('猜数字游戏开始，请输入数字：'))
        if number > right:
            print('您输入的数字大了！')
        else:
            print('您输入的数字小了！')
        if number == right:
            print('恭喜，您猜对了！')
            print('游戏结束，再见！')
            
